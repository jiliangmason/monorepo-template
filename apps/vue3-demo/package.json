      "mockjs": "^1.1.0",
      "pinia": "^2.0.21",
      "promise.allsettled": "^1.0.5",
      "vue": "^3.2.38",
      "vue-router": "^4.1.5"
    },
    "devDependencies": {
      "@types/lodash": "^4.14.186",
      "@types/node": "^16.11.56",
      "@vitejs/plugin-legacy": "^2.2.0",
      "@vitejs/plugin-vue": "^3.0.3",
      "@vue/tsconfig": "^0.1.3",
      "autoprefixer": "^10.4.12",
      "less": "^4.1.2",
      "less-loader": "^10.2.0",
      "npm-run-all": "^4.1.5",
      "postcss": "^8.4.18",
